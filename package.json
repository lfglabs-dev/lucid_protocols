{
  "name": "lucid_protocols",
  "version": "1.0.0",
  "description": "A TypeScript repository of whitelisted protocols inspired by DefiLlama-Adapters",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "fetch": "ts-node src/scripts/fetchContracts.ts",
    "build-registry": "ts-node src/scripts/buildRegistry.ts",
    "fetch:clear-signing": "ts-node src/scripts/fetchClearSigningMetadata.ts",
    "build:clear-signing-registry": "ts-node src/scripts/buildClearSigningRegistry.ts",
    "clear-signing": "npm run fetch:clear-signing && npm run build:clear-signing-registry",
    "clean": "rm -rf dist",
    "add:hardcoded": "ts-node src/scripts/addHardcodedProtocols.ts",
    "fetch:euler": "ts-node src/scripts/fetchEulerContracts.ts",
    "fetch:pendle": "ts-node src/scripts/fetchPendleContracts.ts"
  },
  "keywords": [
    "defi",
    "protocols",
    "blockchain",
    "ethereum",
    "smart-contracts"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.7",
    "octokit": "^3.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
