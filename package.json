{
  "name": "lucid_protocols",
  "version": "1.0.0",
  "description": "A TypeScript repository of whitelisted protocols inspired by DefiLlama-Adapters",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "fetch:exclude": "ts-node src/scripts/fetchExclusionList.ts",
    "parse": "ts-node src/scripts/parseProtocols.ts",
    "build-registry": "ts-node src/utils/buildRegistry.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "defi",
    "protocols",
    "blockchain",
    "ethereum",
    "smart-contracts"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.7",
    "octokit": "^3.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
